@Article{keeffe_simplified_1996,
  Title                    = {A simplified screening test for identifying people with low vision in developing countries},
  Author                   = {Keeffe, J E and Lovie-Kitchin, J E and Maclean, H and Taylor, H R},
  Year                     = {1996},
  Number                   = {5},
