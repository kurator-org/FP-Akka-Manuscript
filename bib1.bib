@Article{hafen_sevenless_1987,
  Title                    = {Sevenless, a cell-specific homeotic gene of {Drosophila}, encodes a putative transmembrane receptor with a tyrosine kinase domain},
  Author                   = {Hafen, E and Basler, K and Edstroem, J E and Rubin, G M},
  Year                     = {1987},
  Number                   = {4797},
  Pages                    = {55--63},
  Url                      = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=2882603},
  Volume                   = {236},

  Abstract                 = {The determination of cell fates during the assembly of the ommatidia in the compound eye of Drosophila appears to be controlled by cell-cell interactions. In this process, the sevenless gene is essential for the development of a single type of photoreceptor cell. In the absence of proper sevenless function the cells that would normally become the R7 photoreceptors instead become nonneuronal cells. Previous morphological and genetic analysis has indicated that the product of the sevenless gene is involved in reading or interpreting the positional information that specifies this particular developmental pathway. The sevenless gene has now been isolated and characterized. The data indicate that sevenless encodes a transmembrane protein with a tyrosine kinase domain. This structural similarity between sevenless and certain hormone receptors suggests that similar mechanisms are involved in developmental decisions based on cell-cell interaction and physiological or developmental changes induced by diffusible factors.},
  Journal                  = {Science},
  Keywords                 = {*embryology, *Genes, *genetics, Amino Acid Sequence, Animal, Base Sequence, Cell Surface, Chromosome Mapping, Cloning, cytology, diagnostic use, DNA Restriction Enzymes, Drosophila melanogaster, embryology, Eye, Gene Expression Regulation, Genes, Genetic, Genetics, Growth Substances, Homeobox, Membrane Proteins, Molecular, Non-U.S. Gov't, Phenotype, P.H.S., Physiology, Protein-Tyrosine Kinase, Receptors, Structural, Support, Transcription, U.S. Gov't}
