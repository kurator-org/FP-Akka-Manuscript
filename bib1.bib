@Article{moreau_provenance_2010,
  Title                    = {The {Provenance} of {Electronic} {Data}},
  Author                   = {Moreau, Luc and Groth, Paul and Miles, Simon and Vazquez, Javier and Ibbotson, John and Jiang, Sheng and Munroe, Steve and Rana, Omer and Schreiber, Andreas and Tan, Victor and Varga, Laszlo},
