@Article{gautier_-skipping_2000,
  Title                    = {'{The}-skipping' revisited in {French}: programming saccades to skip the article 'les'},
  Author                   = {Gautier, V and O'Regan, J K and Le Gargasson, J F},
  Year                     = {2000},
