@Article{beaman_hiclas:_1999,
  Title                    = {{HICLAS}: {A} taxonomic database systems for comparing biological classification and phylogenetic trees},
  Author                   = {Beaman, J and Luo, Y and Pramanik, Sakti and Zhong, Yang},
  Year                     = {1999},
  Number                   = {2},
