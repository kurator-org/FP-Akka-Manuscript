@Article{mansfield_psychophysics_1996,
  Title                    = {Psychophysics of reading. {XV}: {Font} effects in normal and low vision},
  Author                   = {Mansfield, J S and Legge, G E and Bane, M C},
  Year                     = {1996},
  Number                   = {8},
  Pages                    = {1492--1501},
  Url                      = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=8675391},
  Volume                   = {37},

  Abstract                 = {PURPOSE: Little is known about the effect of font on low-vision reading. In this study, the authors measured the influence of font in reading with normal and low vision. METHODS: Reading acuity, maximum reading speed, and critical print size (the smallest print that can be read with maximum speed) were measured in 50 normal subjects and 42 subjects with low vision. Data were collected using versions of the MNREAD Acuity Chart printed with the Times (proportionally spaced) and Courier (fixed-width) fonts. RESULTS: Reading acuity scores obtained with Courier were better than those obtained with Times for both normal (mean difference, 0.05 logMAR, P {\textless} 0.001) and subjects with low vision (0.09 logMAR, P {\textless} 0.001). Similarly, critical print sizes measured with Courier were smaller than those measured with Times (mean difference, 0.06 logMAR for normal subjects and subjects with low vision, P {\textless} 0.002). Maximum reading speeds for normal subjects were 5\% faster with Times than with Courier (P {\textless} 0.001), but for subjects with low vision, maximum reading speeds were 10\% slower with Times than with Courier (P {\textless} 0.05). For print smaller than the critical print size, the reading speeds of normal subjects and subjects with low vision were substantially slower (by as much as 50\%) for Times than for Courier. CONCLUSIONS: There are small, but significant, advantages of Courier over Times in reading acuity, critical print size, and reading speed for subjects with low vision. For normal subjects, the differences are slighter, with an advantage in reading speed for Times. However, for print sizes close to the acuity limit, choice of font could make a significant difference in both normal and low-vision reading performance.},
  Journal                  = {Invest Ophthalmol Vis Sci},
  Keywords                 = {*physiology, *physiopathology, *Reading, 80 and over, Adult, Aged, Aging, Contrast Sensitivity, etiology, Form Perception, Human, Low, Middle Age, P.H.S., Physiology, Printing, Psychophysics, Size Perception, Support, U.S. Gov't, Vision, Vision Tests, Visual Acuity}
}

